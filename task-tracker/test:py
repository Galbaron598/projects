def longestConsecutive(nums):
    if not nums:
        return 0
        
    nums_set= set(nums)
    longest = 0
    for num in nums_set:
        if num-1 not in nums_set: 
            current_num=num
            strike =1
            while current_num +1 in nums_set:
                current_num +=1
                strike +=1
        longest = max(longest, strike)
                
    return longest
print(longestConsecutive([100, 4, 200, 1, 3, 2]))  # Output: 4 (sequence: 1, 2, 3, 4)